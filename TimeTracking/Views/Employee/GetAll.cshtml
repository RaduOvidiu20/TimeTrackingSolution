@model System.Collections.Generic.List<Core.Entities.Employee>


@{
    ViewBag.Title = "Employees";
    Layout = "_Layout";
}
<h2 class="text-decoration-underline">Employees</h2>
<form asp-controller="Employee" asp-action="GetAll" method="get">
<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Phone number</th>
        <th></th>
    </tr>
    </thead>
    @foreach (Employee employee in ViewBag.Employees)
    {
        <tbody>
        <tr>
            <td>@employee.Name</td>
            <td>@employee.Age</td>
            <td>@employee.Phone</td>
            <td></td>
        </tr>
        </tbody>
    }
</table>
</form>